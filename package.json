{
  "private": true,
  "scripts": {
    "build:demo": "ng build demo",
    "build:lib": "ng build ngx-editor",
    "build": "npm run build:lib && npm run build:demo",
    "postbuild:lib": "node ./scripts/postbuild.js",
    "dev": "ng build ngx-editor --watch --configuration development",
    "demo": "ng serve demo --port 5002 --configuration development",
    "docs": "docsify serve ./docs",
    "ng": "ng",
    "lint": "ng lint",
    "format:md": "prettier **/*.md --write",
    "prerelease": "npm run build:lib",
    "release": "sh ./scripts/release.sh",
    "test:demo": "ng test demo --browsers ChromeHeadless --watch false",
    "test:lib": "ng test ngx-editor --browsers ChromeHeadless --watch false",
    "test": "npm run test:lib && npm run test:demo",
    "prepare": "sh ./scripts/install-husky.sh"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7.20"
  },
  "keywords": [
    "angular-editor",
    "angular-wysiwyg-editor",
    "wysiwyg-editor",
    "ngx-editor",
    "prosemirror",
    "angular-prosemirror",
    "angular-rich-text-editor",
    "angular-rte"
  ],
  "dependencies": {
    "@angular/animations": "~12.2.1",
    "@angular/common": "~12.2.1",
    "@angular/compiler": "~12.2.1",
    "@angular/core": "~12.2.1",
    "@angular/forms": "~12.2.1",
    "@angular/platform-browser": "~12.2.1",
    "@angular/platform-browser-dynamic": "~12.2.1",
    "@angular/router": "~12.2.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.1",
    "@angular-eslint/builder": "^12.3.1",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.3.1",
    "@angular/cli": "~12.2.1",
    "@angular/compiler-cli": "~12.2.1",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@types/prosemirror-commands": "^1.0.4",
    "@types/prosemirror-history": "^1.0.3",
    "@types/prosemirror-inputrules": "^1.0.4",
    "@types/prosemirror-keymap": "^1.0.4",
    "@types/prosemirror-model": "^1.13.2",
    "@types/prosemirror-schema-list": "^1.0.3",
    "@types/prosemirror-state": "^1.2.7",
    "@types/prosemirror-view": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chalk": "^4.1.2",
    "codemirror": "^5.62.3",
    "docsify-cli": "^4.4.3",
    "eslint": "^7.32.0",
    "eslint-config-pegasus": "^2.1.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "inquirer": "^8.1.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
